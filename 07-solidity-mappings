pragma solidity ^0.8.18;

contract MappingsDemo {
    mapping(uint => address) public idToOwner;
    mapping(address => uint) public ownerBalance;

    function assign(uint _id) public {
        idToOwner[_id] = msg.sender;
        ownerBalance[msg.sender]++;
    }

    function getOwner(uint _id) public view returns (address) {
        return idToOwner[_id];
    }
}
